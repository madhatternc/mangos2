-
    name: SMSG_UPDATE_OBJECT
    opcode: 0x00A9
    type: packet
    direction: out
    info: The most complex packet type
    structure:
      - Count:
            type: uint32
      - unknown:
            type: uint8
            const: 0
      - Values:
            type: vector
            vtype: ObjectVector
            structure:
              - TypeFlag:
                    type: uint8
                    const: UPDATE_VALUES
              - GetGUID ():
                    type: uint64
      - Movement:
            type: vector
            vtype: ObjectVector
            structure:
              - TypeFlag:
                    type: uint8
                    const: UPDATE_MOVEMENT
              - GetGUID ():
                    type: uint64
      - CreateObject:
            type: vector
            vtype: ObjectVector
            structure:
              - TypeFlag:
                    type: uint8
                    const: UPDATE_CREATE_OBJECT
              - GetGUID ():
                    type: uint64
              - GetType ():
                    type: uint8
      - if OutOfRange.Length ():
          - TypeFlag:
                type: uint8
                const: UPDATE_OUT_OF_RANGE_OBJECTS
          - OutOfRangeCount:
                type: uint32
      - OutOfRange:
            type: vector
            counter: OutOfRangeCount
            vtype: ObjectVector
            structure:
              - GetGUID ():
                    type: uint64
