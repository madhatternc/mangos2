    name: SMSG_FRIEND_STATUS
    opcode: 0x0068
    type: packet
    info: Sends status of a friend
    direction: out
    structure:
      - Result:
          type: uint8
      - Guid:
          type: guid
      - if Result == FRIEND_ADDED_ONLINE || Result == FRIEND_ONLINE || Result ==  FRIEND_OFFLINE:
          - Area:
              type: uint32
          - Level:
              type: uint32
          - Class:
