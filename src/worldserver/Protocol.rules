-
    name: SMSG_GROUP_LIST
    opcode: 0x007D
    info: Sends a list with all members except the current member
    type: packet
    direction: out
    structure:
      - spacer
      - Memberscount:
          type: uint8
          info: Number of members in the group: Members.Length () + 1
      - spacer
      - Members:
          type: vector
          vtype: MembersVector
          counter: Memberscount
          info: For every member but the one getting sent the msg: Members.Length () = Memberscount - 1
          structure:
            - Slotid:
                type: uint8
                info: starts at 0
            - spacer
            - Membername:
                type: string
            - spacer
            - Guidsno:
                type: uint32
            - spacer
      - unknown1:
          type: uint8
          const: 1
          info: no idea but always 1
      - spacer
      - Leaderguid:
          type: uint32
      - spacer
      - Lootmethord:
          type: uint8
      - Lootmaster:
          type: uint32
      - spacer
      - Lootthreshold:
          type: uint8


