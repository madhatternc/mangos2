-
    name: STRL_SPELL_TARGET
    type: structlet
    struct-name: SPELL_TARGET
    info: this is used at spell target messages
    structure:
        - Targetmask:
             type: uint16
        - if Targetmask & TARGET_FLAG_UNIT || Targetmask & TARGET_FLAG_OBJECT:
             - Targetunit:
                 type: guid
        - if Targetmask & TARGET_FLAG_ITEM:
             - Targetitem:
                 type: guid
        - if Targetmask & TARGET_FLAG_SOURCE_LOCATION:
             - SrcX:
                 type: float
             - SrcY:
                 type: float
             - SrcZ:
                 type: float
        - if Targetmask & TARGET_FLAG_DEST_LOCATION:
             - DestX:
                 type: float
             - DestY:
                 type: float
             - DestZ:
                 type: float
        - if Targetmask & TARGET_FLAG_STRING:
             - Targetstring:
                 type: string

