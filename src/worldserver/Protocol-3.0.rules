--- #YAML:1.0
# World server protocol opcodes client 3.0.2 build 9056
# This is a YAML File: http://yaml4r.sourceforge.net/cookbook/
-
    name: header_start
    type: c++code
    code: |
        #ifndef __OPCODES_H__
        #define __OPCODES_H__

        #include "Sockets.h"
        #include "ProtocolVectors.h"

        const char *GetPacketName (uint Opcode);

-
    name: header_end
    type: c++code
    code: |
        #endif // __OPCODES_H__

-
    name: opcodes_start
    type: c++code
    code: |
        enum Opcodes
        {

-
    name: opcodes_end
    type: c++code
    code: |
        };

-
    name: pktnames_start
    type: c++code
    code: |
        struct
        {
            uint32 Opcode;
            const char *Name;
        } PacketNames [] =
        {

-
    name: pktnames_end
    type: c++code
    code: |
        };

        const char *GetPacketName (uint Opcode)
        {
            for (uint i = 0; i < ARRAY_LEN (PacketNames); i++)
                if (PacketNames [i].Opcode == Opcode)
                    return PacketNames [i].Name;
            return NULL;
        }


-
    name: code_start
    type: c++code
    code: |
        #include "Opcodes.h"

-
    name: code_end
    type: c++code
    code: |
        // --- THE END ---

-
    name: pkthdr_size
    type: c++code
    code: sizeof(uint16)+sizeof(uint16)

-
    name: pkthdr_assemble
    type: c++code
    code: |
        PUT_BE16 (cur, length - 2); cur += sizeof (uint16);
        PUT_LE16 (cur, %(name)s); cur += sizeof (uint16);

