    opcode: 0x0067
    type: packet
    info: Sends a list with all friends
    direction: out
    structure:
      - Count:
            type: uint8
      - Friends:
            type: vector
            counter: Count
            vtype: FriendsVector
            structure:
              - PlayerGUID:
                    type: guid
              - Status:
                    type: uint8
              - if Status != 0:
                  - Area:
                        type: uint32
                  - Level:
                        type: uint32
                  - Class:
                        type: uint32

